    "import textwrap\n",
    "\n",
    "def wrap(string, max_width):\n",
    "    stringa=\"\"\n",
    "    for i in range(0,len(string)):\n",
    "        stringa= stringa + string[i]\n",
    "        if (i +1 ) % max_width == 0:\n",
    "            stringa=stringa +\"\\n\"\n",
    "\n",
    "    return stringa\n",
    "\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    string, max_width = input(), int(input())\n",
    "    result = wrap(string, max_width)\n",
    "    print(result)"
   ]
