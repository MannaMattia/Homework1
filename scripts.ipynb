    "def mutate_string(string, position, character):\n",
    "    stringa=\"\"\n",
    "    for i in range(0,len(string)):\n",
    "        if i == position:\n",
    "            stringa=stringa + character\n",
    "        else:\n",
    "            stringa=stringa + string[i]\n",
    "    \n",
    "    return stringa\n",
    "\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    i, c = input().split()\n",
    "    s_new = mutate_string(s, int(i), c)\n",
    "    print(s_new)"
