    "import math\n",
    "import os\n",
    "import random\n",
    "import re\n",
    "import sys\n",
    "\n",
    "\n",
    "def kangaroo(x1, v1, x2, v2):\n",
    "    if x1 < x2 and v1 < v2:\n",
    "        return \"NO\"\n",
    "    #calcolo mcm\n",
    "    elif x1 < x2 and v1==v2:\n",
    "        return \"NO\"\n",
    "    \n",
    "    t=(x2-x1)/(v1-v2)\n",
    "\n",
    "    \n",
